typedef struct (int strlen) {
    byte data[strlen * 2];
} acnhstring;

typedef struct (int size) {
    int uid;
    acnhstring str(size);
} uid_str;

typedef struct {
    byte a;
    byte b;
    byte c;
} color;

typedef struct {
    int value;
    ushort adjust;
    byte shift;
    byte checksum;
} encrypted_int32;
